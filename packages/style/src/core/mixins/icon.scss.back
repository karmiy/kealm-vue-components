@font-face {
  font-family: $icon-family;
  src:  url('./fonts/salus-icon.eot?njldcd');
  src:  url('./fonts/salus-icon.eot?njldcd#iefix') format('embedded-opentype'),
  url('./fonts/salus-icon.ttf?njldcd') format('truetype'),
  url('./fonts/salus-icon.woff?njldcd') format('woff'),
  url('./fonts/salus-icon.svg?njldcd#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}
[class^="salus-icon-"], [class*=" salus-icon-"]{
  font-family: $icon-family !important;
  display: inline-block;
  font-size: $font-size-normal;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
}
