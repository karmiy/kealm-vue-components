/* 加载图标*/
//默认样式
.salus-icon-loading{
  animation: ws-rotate 1.6s infinite linear;
  font-size: 26px;
}
.salus-icon-loading::before{
  content: "\e948";
  background: linear-gradient(to bottom right, #1394FF, #16EBFE);
  -webkit-background-clip: text;
  color: transparent;
}
//在默认样式的基础上修改样式
.salus-icon-loading-white::before{
  background: linear-gradient(to bottom right, #fff, rgba(255, 255, 255, 0.2) );
  -webkit-background-clip: text;
  color: transparent;
}
.salus-icon-loading-gray::before{
  background: linear-gradient(to bottom right, #404040, rgba(255, 255, 255, 0.2) );
  -webkit-background-clip: text;
  color: transparent;
}
.salus-icon-online:before {
  content: "\e950";
}
.salus-icon-child-account:before {
  content: "\e951";
}
.salus-icon-offline:before {
  content: "\e956";
}
.salus-icon-link-o:before {
  content: "\e95a";
}
.salus-icon-video-o:before {
  content: "\e95b";
}

.salus-icon-52px{
  font-size: 52px;
}

//设置旋转动画
@-webkit-keyframes ws-rotate {
  from {
    -webkit-transform: rotate(0deg);
  }
  to {
    -webkit-transform: rotate(360deg);
  }
}
@keyframes ws-rotate {
  from {
    transform: rotate(0deg);
  }
  to {
    transform: rotate(360deg);
  }
}
